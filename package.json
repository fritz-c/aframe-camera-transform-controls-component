    "prepublish": "npm run dist",
    "start": "npm run dev",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fritz-c/aframe-camera-transform-controls-component.git"
  },
  "keywords": [
    "aframe",
