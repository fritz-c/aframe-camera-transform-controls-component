    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "camera-transform-controls"
  ],
  "author": "Fernando Serrano <fernandojsg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fritz-c/aframe-camera-transform-controls-component/issues"
  },
  "homepage": "https://github.com/fritz-c/aframe-camera-transform-controls-component#readme",
  "devDependencies": {
    "aframe": "*",
    "budo": "^8.2.2",
    "randomcolor": "^0.4.4",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.3.4"
  }
}
