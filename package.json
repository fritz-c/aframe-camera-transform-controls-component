{
  "name": "aframe-camera-transform-controls-component",
  "version": "1.0.0",
  "description": "A Camera Transform Controls component for A-Frame.",
  "main": "dist/aframe-camera-transform-controls-component.js",
  "unpkg": "dist/aframe-camera-transform-controls-component.min.js",
  "scripts": {
    "dev": "budo index.js:dist/aframe-camera-transform-controls-component.min.js --port 7000 --live --open",
